{
    "compilerOptions": {
        "target": "ES2023",
        "module": "ESNext",
        "rootDir": ".",
        "outDir": "./dist",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*", "server.ts"]
}
